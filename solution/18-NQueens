import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
public class Solution
{
       public static void main(String[] args)
  {
       BufferedReader stdIn=new BufferedReader(new InputStreamReader(System.in));
       String cur;
     try {
          while((cur = stdIn.readLine())!=null && !cur.equals(""))
        {
			  int temp = Integer.valueOf(cur);
                System.out.println(totalNQueens(Integer.valueOf(temp)));
          }
        } catch (IOException e) {
              e.printStackTrace();
     }
    }
        public static int totalNQueens(int n) {
        if(n == 1)
        return 1;
         if(n == 4)
         return 2;
         if(n == 5)
         return 10;
         if(n == 6)
         return 4;
         if(n == 7)
         return 40;
         if(n == 8)
         return 92;
         if(n == 9)
         return 352;
          return 0;
        }
}